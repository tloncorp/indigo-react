    "formik": "../node_modules/formik",
    "react": "../node_modules/react"
  },
  "author": "Gavin Atkinson",
  "license": "MIT",
  "devDependencies": {
    "@types/react-dom": "^16.9.8",
    "@types/yup": "^0.29.5",
