    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@tlon/indigo-light": "^1.0.4",
    "@types/react-router-dom": "^5.1.5",
    "local-indigo-react": "file:..",
    "react-dom": "^16.13.1",
